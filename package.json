{
  "name": "essence",
  "version": "0.0.1",
  "description": "essence - basic material design UI framework",
  "private": true,
  "keywords": [
    "essence",
    "basic",
    "frontend",
    "ui",
    "framework",
    "less"
  ],
  "paths": {
