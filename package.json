    "grunt-contrib-less": "^0.12.0",
    "grunt-contrib-uglify": "^0.8.1",
    "grunt-contrib-watch": "^0.6.1",
    "grunt-lesslint": "^1.1.13",
    "load-grunt-configs": "^0.4.1",
    "load-grunt-tasks": "^1.0.0",
    "react": "^0.12",
    "reactify": "~0.17.1"
